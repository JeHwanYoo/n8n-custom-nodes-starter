{
  "name": "rag-price-tracker-nodes",
  "version": "1.0.0",
  "description": "RAG Price Tracker - n8n Custom Nodes",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint nodes/**/*.ts",
    "lint:fix": "eslint nodes/**/*.ts --fix",
    "format": "prettier --write \"nodes/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"nodes/**/*.{ts,js,json}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
